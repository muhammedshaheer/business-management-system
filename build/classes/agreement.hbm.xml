<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
	<hibernate-mapping>  
 		<class name="com.bms.domain.Agreement" table="agreement">  
    		<id name="agreementId">  
     			<generator class="increment"></generator>
    		</id>  
   
    		<property name="prospectId"/>  
    		<property name="dateOfAgreement"/> 
    		<property name="finishingDate"/> 
    		<property name="estimateAmount"/>
            <property name="periodOfContract"/>
            <property name="terminationConditions"/>
            <property name="linkToFile"/>
            <property name="confirm"/>
            
  		</class>  
            
	</hibernate-mapping>  
 